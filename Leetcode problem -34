class Solution:
    def findFirst(self, nums, n, x):
        low, high = 0, n-1
        first =-1
        while low<=high:
            mid =(low+high)//2
            if nums[mid]==x:
                first = mid
                high =mid-1
            elif nums[mid]<x:
                low = mid+1
            else:
                high = mid-1
        return first
    def findLast(self, nums, n, x):
        low, high =0, n-1
        last =-1
        while low<= high:
            mid =(low + high)//2
            if nums[mid]==x:
                last = mid
                low = mid + 1
            elif nums[mid] > x:
                high = mid -1
            else:
                low = mid + 1
        return last
    def searchRange(self, nums: List[int], target: int) -> List[int]:
        n=  len(nums)
        first = self.findFirst(nums, n,  target)
        last = self.findLast(nums, n, target)
        return [first, last]
